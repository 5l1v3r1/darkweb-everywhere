<ruleset name="I2p2web Eepsite" default_off="i2p rule, might cause conflict">
	<!-- Some of these hosts have not been up for a while, but I am including them for legacy reasons. -->
	<target host="*.i2p.to" />
	<target host="*.i2p.in" />
	<target host="*.i2p.us" />
	<target host="*.i2p.rocks" />
	<target host="*.i2p.xyz" />
	<rule from="^https?://((\w+\.)+)i2p\.(to|in|us|rocks|xyz)/" to="http://$1.i2p/" />
</ruleset>
